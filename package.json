    "@sendgrid/mail": "7.7.0",
    "@svgr/webpack": "6.3.1",
    "axios": "0.27.2",
    "bootstrap": "5.2.1",
    "next": "12.3.1",
    "next-seo": "5.5.0",
    "react": "18.2.0",
    "react-bootstrap": "2.5.0",
    "react-calendly": "4.1.0",
    "react-dom": "18.2.0",
    "react-particles": "2.3.2",
    "react-tsparticles": "2.3.2",
    "twind": "0.16.17"
